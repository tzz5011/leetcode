class Solution:
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x<0:
            return False
        num=str(x);
        rev="";
        for i in range(len(num)):
            rev=num[i]+rev;
        if rev == "":
            return "no number entered";
        if rev == num:
            return True
        else:
            return False
        
