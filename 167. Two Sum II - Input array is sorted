class Solution:
    def twoSum(self, numbers, target):
        """
        :type numbers: List[int]
        :type target: int
        :rtype: List[int]
        """
        hashdict={};
        for i, num in enumerate(numbers):
            complement = target-num;
            if complement in hashdict:
                return[hashdict[complement]+1, i+1];
            hashdict[num]=i;
